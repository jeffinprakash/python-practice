e_count=0
o_count=0
for i in range(1,10):
  if(i%2==0):
    e_count = e_count+1
  else:
       o_count=o_count+1
print("even number count", e_count)
print("odd number count", o_count)

----------------------------------------------------------------------------------------------------

#2. Count the number which are divisible 3 and 5 within the range of 1 to 100

count=0
for i in range(1,101):
   if (i%3==0 and i%5==0):
       count=count+1
print("The numbers divisible by 3 and 5 are: ", count)
       
--------------------------------------------------------------------------------------------------------

#3. Find the sum of natural numbers between 1 to 5?

sum=0
for i in range(1,6):
    sum=sum+i
print("sum of natural numbers are:", sum)
    
----------------------------------------------------------------------------------------------------------


